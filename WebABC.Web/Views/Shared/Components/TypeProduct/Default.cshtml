@model IEnumerable<TypeProductDto>
    <div class="filter-widget">
        <h4 class="fw-title">Categories</h4>
        <ul class="filter-catagories">
            @foreach (var item in Model)
            {
                <li><a asp-action="Index" asp-route-cat="@item.Name">@item.Name</a></li>
            }
        </ul>
    </div>